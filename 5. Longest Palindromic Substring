class Solution {
    public String longestPalindrome(String s) {
        int[] l = new int[s.length()];
        for (i=0; i<s.length(); i++){
            for (j=s.length; j>0; j--){
                while(s[i]==s[j])
                {
                    j=j+1;
                    i=i+1;
                    l[i]=l[i]+1;
                }
            }
        }
        int lmax = 1;
        int maxi = 0;
        string maxsub =null;
        for (i=0; i<s.length(); i++){
            if (s[i]>=lmax){
                lmax = s[i];
                maxi = i;
            }
            
        }
        return 
    }
}
